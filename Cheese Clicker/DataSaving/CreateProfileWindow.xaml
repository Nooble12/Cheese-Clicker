<Window x:Class="Cheese_Clicker.DataSaving.CreateProfileWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cheese_Clicker.DataSaving"
        mc:Ignorable="d"
        Title="CreateProfileWindow" Height="450" Width="800">
    <Grid>

        <Button Content="ConfirmButton" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,260,0,0"
             Width="150" Click="ConfirmButton_Click"/>
        <TextBox
         Name="ProfileNameLabel"
         HorizontalAlignment="Center"
         VerticalAlignment="Center"
         Text="Enter Profile Name"
            HorizontalContentAlignment="Center"
            VerticalContentAlignment="Center"
         Width="331" Height="75"
         />
    </Grid>
</Window>
