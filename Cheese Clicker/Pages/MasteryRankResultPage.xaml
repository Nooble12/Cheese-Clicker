<Page x:Class="Cheese_Clicker.ModifierClasses.MasteryRankResultPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Cheese_Clicker.ModifierClasses"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Background="Gray"
      Title="MasteryRankResultPage">

    <Grid>
        <Button HorizontalAlignment="Left" VerticalAlignment="Bottom" Content="Back" Margin="10,0,0,10" Click="BackToGamePageButton_Click" Width="97" Height="45" Template="{DynamicResource CurvedButton}"/>
        <Label Content="Results " FontSize="30" HorizontalAlignment="Left" VerticalAlignment="Top"></Label>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Label Name="MasteryRankLabel" Content="Mastery Rank: " FontSize= "30"></Label>
            <Label Name="ResultsLabel" Content="Results Here" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"></Label>
        </StackPanel>
    </Grid>
</Page>
